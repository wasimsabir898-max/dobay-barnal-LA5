<script>
let yes = localStorage.getItem("yesVotes") || 0;
let no = localStorage.getItem("noVotes") || 0;

document.getElementById("yes").innerText = yes;
document.getElementById("no").innerText = no;

function voteYes(){
  yes++;
  localStorage.setItem("yesVotes", yes);
  document.getElementById("yes").innerText = yes;
}
function voteNo(){
  no++;
  localStorage.setItem("noVotes", no);
  document.getElementById("no").innerText = no;
}
</script>

